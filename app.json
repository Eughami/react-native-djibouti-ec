{
  "expo": {
    "name": "Aaway",
    "scheme": "eughami",
    "slug": "djibouti-ec",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#FF8C00"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FF8C00",
      "dark": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#d3792a"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.eughami.djiboutiec",
      "versionCode": 3,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "045def02-df14-4d9f-8def-35afc0bfb05d"
      }
    },
    "owner": "eughami",
    "plugins": [
      "expo-localization",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 31,
            "targetSdkVersion": 31,
            "enableProguardInReleaseBuilds": true
          }
        }
      ]
    ]
  }
}
